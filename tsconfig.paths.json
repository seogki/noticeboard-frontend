{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@ui/*": ["./src/ui/*"],
      "@assets/*": ["./src/assets/*"],
      "@network/*": ["./src/network/*"],
      "@interface/*": ["./src/interface/*"],
      "@redux/*": ["./src/redux/*"],
      "@components/*": ["./src/ui/components/*"],
      "@utils/*": ["./src/utils/*"]
    },
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  }
}
